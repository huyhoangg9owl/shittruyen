{
	"arrowParens": "always",
	"bracketSpacing": true,
	"jsxSingleQuote": false,
	"printWidth": 120,
	"proseWrap": "preserve",
	"quoteProps": "as-needed",
	"semi": true,
	"singleQuote": true,
	"tabWidth": 4,
	"trailingComma": "none",
	"useTabs": true,
	"vueIndentScriptAndStyle": false,
	"endOfLine": "lf",
	"plugins": [
		"@trivago/prettier-plugin-sort-imports",
		"prettier-plugin-organize-imports",
		"prettier-plugin-tailwindcss"
	]
}
