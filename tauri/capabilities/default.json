{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "path:default",
    "event:default",
    "window:default",
    "webview:default",
    "app:default",
    "resources:default",
    "image:default",
    "menu:default",
    "tray:default",
    {
      "identifier": "http:default",
      "allow": [
        "http://*/*",
        "https://*/*"
      ]
    },
    "dialog:default",
    "http:default"
  ]
}